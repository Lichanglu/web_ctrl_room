<script>
function setLanguageType(type) {
	if(type) {
		jQuery('#changeLanguage').val(type);
	}
}
jQuery(function() {
	//初始化抬头信息
	initTopInfo();
	//初始化表单验证
	initFormValidation();
	//美化首页表单
	//formBeautify();
	//初始化菜单导航
	initPageMenu();
	//初始化窗口缩放事件
	//jQuery(window).resize(function() {
		//formBeautify();
	//});
	//初始化语言切换事件
	jQuery("#changeLanguage").change(function() {
		var local = jQuery(this).val();
		var data = {"actioncode": "24", "local": local};
		jQueryAjaxCmd({
			"data": data,
			success: function(ret) {
				eval(ret);
			}
		});
	});
	
});
</script> 

