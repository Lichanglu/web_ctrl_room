jQuery(function() {
	initFormValidation();

	jQuery('#saveBtn').click(function() {
		var validate = validateFormByID('wmform');
		if(validate) {
			var data = {"actioncode": "140", "username": "admin"};
			var params = getFormItemValue('wmform');
			if(params) {
				jQuery.extend(data, params);
			}
			jQueryAjaxCmd({
				"data": data,
				success: function(ret) {
					alert(ret);
					jQuery('#menub').click();
				}
			});
			
		} else {
			return false;
		}
	});
	
	jQuery('#saveBtn2').click(function() {
		var validate = validateFormByID('wmform2');
		if(validate) {
			var data = {"actioncode": "141", "username": "guest"};
			var params = getFormItemValue('wmform2');
			if(params) {
				jQuery.extend(data, params);
			}
			jQueryAjaxCmd({
				"data": data,
				success: function(ret) {
					alert(ret);
					jQuery('#menub').click();
				}
			});
			
		} else {
			return false;
		}
	});
	
	jQuery(':text, :password').keydown(function(evt) {
		if(evt.keyCode == '13') {
			jQuery('#saveBtn').click();
		}
	});
	
});
</script> 